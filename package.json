{
  "name": "setup-packer",
  "version": "1.0.0",
  "author": "Sora Morimoto <sora@morimoto.io>",
  "private": true,
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build": "esbuild src/index.ts --bundle --outdir=dist --target=node12 --platform=node"
  },
  "license": "ISC",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "@actions/io": "1.0.2",
    "@actions/tool-cache": "1.6.1"
  },
  "devDependencies": {
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "esbuild": "0.8.16",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-simple-import-sort": "6.0.1",
    "prettier": "2.2.0",
    "typescript": "4.1.2"
  }
}
